<Window x:Class="skeleton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:skeleton"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="640">
    <Grid x:Name="LayoutRoot" Margin="0,0,2,8">
        <Image Name="colorimage" Height="480" Width="640"/>
        <Canvas Name="Layout" Height="480" Width="640">
            <TextBox x:Name="TextBox" Height="59" Canvas.Left="38" TextWrapping="Wrap" Text="TextBox" Canvas.Top="46" Width="122"/>
            <TextBox x:Name="tr" Height="23" Canvas.Left="40" TextWrapping="Wrap" Text="TextBox" Canvas.Top="110" Width="120"/>
        </Canvas>
    </Grid>
</Window>
